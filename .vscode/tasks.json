{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"command": "build",
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"args": [
				"--all-targets"
			],
			"presentation": {
				"clear": true
			},
			"problemMatcher": [
				"$rustc"
			],
			"group": "build",
			"label": "rust: cargo build"
		}
	]
}